{
    "compilerOptions": {
      "target": "ES6", 
      "module": "commonjs", 
      "strict": true, 
      "esModuleInterop": true, 
      "skipLibCheck": true, 
      "forceConsistentCasingInFileNames": true, 
      "outDir": "./dist", 
      "resolveJsonModule": true, 
      "isolatedModules": true, 
      "noImplicitAny": true, 
      "moduleResolution": "node", 
      "skipDefaultLibCheck": true 
    },
    "include": ["src/**/*"], 
    "exclude": ["node_modules"]
  }
  